{
    "name": "X-AsenaDuplicated",
    "version":"4.0.0",
    "description": "X-asena Multi Device whatsappbot",
    "logo": "https://i.hizliresim.com/6Bbj7i.jpg",
    "keywords": ["whatsapp"],
    "repository": "https://github.com/V1P3R-X/X-AsenaDuplicated",
    "website": "https://github.com/V1P3R-X/X-AsenaDuplicated",
    "success_url": "https://github.com/V1P3R-X/X-AsenaDuplicated",
    "stack": "container",
    "env": {
        "ANTI_LINK": {
            "description": "true/false",
            "required": false
        },
        "ANTILINK_ACTION": {
            "description": "kick",
            "required": false
        },
        "RMBG_KEY": {
            "description": "Remove bg api key ",
            "required": false
        },
        "SESSION_ID": {
            "description": "Your Session ID",
            "required": true
        },
        "HANDLERS": {
            "description": "prefix for commands, enter false for no prefix",
